NODE_ENV=development
APP_ENV=local
PORT=3001

DATABASE_URL=file:./data/prostyle.local.db

SQS_QUEUE_URL=http://localhost:4566/000000000000/prostyle-analysis-local
SQS_DLQ_URL=http://localhost:4566/000000000000/prostyle-analysis-local-dlq
SQS_MAX_ATTEMPTS=5
SQS_RETRY_BASE_MS=2000
QUEUE_ADAPTER_MODE=sqlite

S3_BUCKET=prostyle-strength-finder-local
AWS_REGION=us-east-1
S3_ENDPOINT_OVERRIDE=http://localhost:4566

COGNITO_USER_POOL_ID=us-east-1_localpool
COGNITO_CLIENT_ID=local-client-id
COGNITO_ISSUER=https://cognito-idp.us-east-1.amazonaws.com/us-east-1_localpool
COGNITO_AUDIENCE=local-client-id
FRONTEND_AUTH_MODE=disabled
COGNITO_HOSTED_UI_BASE_URL=https://your-local-domain.auth.us-east-1.amazoncognito.com
COGNITO_REDIRECT_PATH=/api/auth/callback
COGNITO_POST_LOGOUT_REDIRECT_PATH=/
FRONTEND_SESSION_SECRET=replace-with-long-random-secret
FRONTEND_SESSION_COOKIE_NAME=prostyle_frontend_session
LOCAL_AUTH_BYPASS_SUBJECT=frontend-local-user
LOCAL_AUTH_BYPASS_EMAIL=
AUTH_JWT_VERIFICATION_MODE=insecure
AUTH_JWKS_CACHE_TTL_SEC=600
DEFAULT_STANDARD_MODEL_VERSION=7
DEFAULT_NIJI_MODEL_VERSION=7
TRAIT_INFERENCE_MODE=llm
OPENAI_MODEL=gpt-5-mini
OPENAI_BASE_URL=https://api.openai.com/v1

LOG_LEVEL=debug
SERVICE_NAME=prostyle-api
LOG_INCLUDE_CORRELATION_IDS=true

NEXT_PUBLIC_API_BASE_URL=http://localhost:3001/v1
NEXT_PUBLIC_APP_BASE_URL=http://127.0.0.1:3000
