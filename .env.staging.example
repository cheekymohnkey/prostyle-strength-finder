NODE_ENV=production
APP_ENV=staging
PORT=3001

DATABASE_URL=file:/var/lib/prostyle/prostyle.staging.db

SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/prostyle-analysis-staging
SQS_DLQ_URL=https://sqs.us-east-1.amazonaws.com/123456789012/prostyle-analysis-staging-dlq
SQS_MAX_ATTEMPTS=5
SQS_RETRY_BASE_MS=2000
QUEUE_ADAPTER_MODE=sqs

S3_BUCKET=prostyle-strength-finder-staging
AWS_REGION=us-east-1
S3_ENDPOINT_OVERRIDE=

COGNITO_USER_POOL_ID=us-east-1_stagingpool
COGNITO_CLIENT_ID=staging-client-id
COGNITO_ISSUER=https://cognito-idp.us-east-1.amazonaws.com/us-east-1_stagingpool
COGNITO_AUDIENCE=staging-client-id
AUTH_JWT_VERIFICATION_MODE=jwks
AUTH_JWKS_CACHE_TTL_SEC=600

LOG_LEVEL=info
SERVICE_NAME=prostyle-api
LOG_INCLUDE_CORRELATION_IDS=true

NEXT_PUBLIC_API_BASE_URL=https://staging-api.prostyle.example.com/v1
